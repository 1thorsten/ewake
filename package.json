{
	"name": "ewake",
	"version": "1.0.0",
	"description": "Web application that manages computers to wake up via WakeOnLan",
	"private": true,
	"scripts": {
		"increase_version": "npm version patch",
		"build:dist": "webpack --config webpack/webpack.prod.js --progress",
