		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^7.0.0",
		"raw-loader": "^4.0.2",
		"terser-webpack-plugin": "^5.0.3",
		"ts-loader": "^8.0.12",
		"typescript": "^4.1.3",
		"webpack": "^5.11.1",
		"webpack-cli": "^4.3.0",
		"webpack-merge": "^5.7.3"
	},
	"dependencies": {
		"dashdash": "^2.0.0",
		"dayjs": "^1.9.8",
		"request-ip": "^2.1.3",
